<Window x:Class="WpfRhinoInsideExample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfRhinoInsideExample"
        xmlns:rt="clr-namespace:RhinoToolkit.Behaviors;assembly=RhinoToolkit"
        xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
        xmlns:pg="clr-namespace:WpfRhinoInsideExample.Pages"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <i:Interaction.Behaviors>
        <rt:RhinoAttachBehavior/>
    </i:Interaction.Behaviors>
    <TabControl>
        <TabItem Header="rhino data render">
            <pg:Render/>
        </TabItem>
        <TabItem Header="call gh">
            <pg:CallGH/>
        </TabItem>

    </TabControl>
</Window>
